			{
				"language": "ucf",
				"scopeName": "source.ucf",
				"path": "./syntaxes/ucf.tmLanguage"
			}
		],
		"snippets": [
			{
				"language": "verilog",
				"path": "./snippets/verilog.json"
			}
		]
	},
