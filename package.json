		"vscode": "^0.10.1"
	},
	"categories": [
		"Languages",
		"Snippets"
	],
	"contributes": {
		"languages": [
			{
				"id": "verilog",
				"aliases": [
					"Verilog",
					"verilog"
				],
				"extensions": [
					".v",
					".vh"
				],
				"configuration": "./verilog.configuration.json"
			},
			{
				"id": "ucf",
				"aliases": [
					"UCF",
					"Ucf"
				],
				"extensions": [
					".ucf"
				],
				"configuration": "./ucf.configuration.json"
			}
		],
		"grammars": [
			{
				"language": "verilog",
				"scopeName": "source.verilog",
